package org.studyeasy.hibernate.entity;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;

@Entity
@Table(name="Users")
public class Users {

	@Id
	@Column(name="user_ID")
	int userID;
	@Column(name="username")
	String username;
	@Column(name="password")
	String password;
	String firstName;
	String lastName;
	
}
